{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "alaouy/youtube": "^2.2",
        "anandsiddharth/laravel-paytm-wallet": "^1.0",
        "boyhagemann/waveform": "0.1.x-dev",
        "braintree/braintree_php": "^5.3",
        "cartalyst/stripe-laravel": "^12.0",
        "devmarketer/easynav": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "instamojo/instamojo-php": "^0.4.0",
        "intervention/image": "^2.5",
        "jackiedo/dotenv-editor": "^1.2",
        "laravel/framework": "^7.22.2",
        "laravel/helpers": "^1.5",
        "laravel/passport": "~9.0",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "laravelcollective/html": "^6.1",
        "laravolt/avatar": "^4.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nesbot/carbon": "^2.54",
        "nwidart/laravel-modules": "^7.1",
        "orangehill/iseed": "^3.0",
        "overtrue/laravel-follow": "2.0.1",
        "paypal/rest-api-sdk-php": "^1.14",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "razorpay/razorpay": "^2.5",
        "revolution/socialite-amazon": "^1.1",
        "seshac/otp-generator": "^0.0.6",
        "spatie/laravel-newsletter": "^4.8",
        "stevebauman/purify": "^3.0",
        "stripe/stripe-php": "^8.5",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "torann/currency": "^1.1",
        "tzsk/payu": "^4.0",
        "unicodeveloper/laravel-paystack": "^1.0",
        "uxweb/sweet-alert": "^2.0",
        "wapmorgan/media-file": "^0.1.4",
        "yajra/laravel-datatables-oracle": "~9.0",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files" : [
            "app/helpers.php",
            "app/models.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
